{
  "name": "common-tg-service",
  "version": "1.0.65",
  "description": "Common Telegram service for NestJS applications",
  "author": "",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "nestjs",
    "telegram",
    "common",
    "service"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:webpack": "node dist/index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "webpack-dev": "webpack --watch --mode development",
    "webpack": "webpack --mode  production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "upload-cloudinary": "node upload-build.js",
    "build:webpack": "npm i && npm run webpack -- --config ./webpack-prod.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.0",
    "adm-zip": "^0.5.0",
    "axios": "^1.8.4",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.0.0",
    "https": "^1.0.0",
    "imap": "^0.8.0",
    "mongoose": "^8.2.2",
    "node-fetch": "^2.7.0",
    "rxjs": "^7.0.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^5.0.0",
    "telegram": "^2.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.0",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@types/express": "4.17.21",
    "@types/imap": "0.8.40",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.28",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/multer": "^1.4.12"
  }
}
